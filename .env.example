# CV-CUE API Configuration
# Copy this file to .env and fill in your actual values

# CV-CUE API Key ID (required)
CV_CUE_KEY_ID=your_key_id_here

# CV-CUE API Key Value (required)
CV_CUE_KEY_VALUE=your_key_value_here

# CV-CUE Client ID (required)
CV_CUE_CLIENT_ID=your_client_id_here

# CV-CUE API Base URL (optional, defaults to production)
CV_CUE_BASE_URL=https://api.cvcue.com
